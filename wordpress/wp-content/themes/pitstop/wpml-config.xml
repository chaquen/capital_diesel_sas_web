<wpml-config>
    <custom-fields>
		<custom-field action="ignore">blog_post_per_page</custom-field>			
		<custom-field action="ignore">cs_homepage_bgcolor</custom-field>			
		<custom-field action="translate">homepage_bgimage</custom-field>			
		<custom-field action="translate">homepage_slider</custom-field>			
		<custom-field action="translate">post_custom</custom-field>			
		<custom-field action="translate">post_gallery</custom-field>			
		<custom-field action="ignore">post_image</custom-field>			
		<custom-field action="translate">post_quote_content</custom-field>			
		<custom-field action="translate">post_quote_source</custom-field>			
		<custom-field action="translate">post_types_select</custom-field>			
		<custom-field action="translate">post_video</custom-field>			
		<custom-field action="ignore">post_video_height</custom-field>			
		<custom-field action="ignore">post_video_href</custom-field>			
		<custom-field action="ignore">post_video_width</custom-field>			
		<custom-field action="translate">sequence_upload</custom-field>			
		<custom-field action="ignore">slide_template</custom-field>			
		<custom-field action="translate">vc_teaser</custom-field>			
		<custom-field action="translate">_menu_item_classes</custom-field>			
		<custom-field action="translate">_menu_item_menu_item_parent</custom-field>			
		<custom-field action="translate">_menu_item_object</custom-field>			
		<custom-field action="ignore">_menu_item_object_id</custom-field>			
		<custom-field action="translate">_menu_item_orphaned</custom-field>			
		<custom-field action="translate">_menu_item_target</custom-field>			
		<custom-field action="translate">_menu_item_type</custom-field>			
		<custom-field action="translate">_menu_item_url</custom-field>
		<custom-field action="translate">_menu_item_xfn</custom-field>
		<custom-field action="translate">_page_portfolio_cat</custom-field>
		<custom-field action="ignore">_page_portfolio_num_items_page</custom-field>			
		<custom-field action="translate">_portfolio_type</custom-field>
		<custom-field action="ignore">_wpb_vc_js_interface_version</custom-field>		
		<custom-field action="ignore">_wpb_vc_js_status</custom-field>
		<custom-field action="translate">_wp_attachment_context</custom-field>			
		<custom-field action="translate">_wp_attachment_custom_header_last_used_tofito</custom-field>	
		<custom-field action="translate">_wp_attachment_image_alt</custom-field>		
		<custom-field action="ignore">_wp_attachment_is_custom_background</custom-field>			
		<custom-field action="ignore">_wp_attachment_is_custom_header</custom-field>			
		<custom-field action="translate">_wp_trash_meta_status</custom-field>			
		<custom-field action="translate">_wp_trash_meta_time</custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1">portfolio</custom-type>
    </custom-types>    
    <taxonomies>
        <taxonomy translate="1">portfolio_category</taxonomy>
    </taxonomies>    
    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>    
        <key name="icl_language_switcher_sidebar">0</key>  
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>    
    </language-switcher-settings>    
</wpml-config>


